{
  "name": "sei-agent-kit",
  "version": "0.0.3",
  "description": "A package for building AI agents on the SEI blockchain",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "tsx test/index.ts",
    "citrex": "tsx test/citrex-test.ts",
    "test-memory": "tsx test-memory.ts",
    "test-memory-agent": "tsx test-memory-agent.ts",
    "test-rag-agent": "tsx test-rag-agent.ts",
    "test-memory-db": "tsx test-memory-db.ts",
    "test-memory-aware-tool": "tsx test-memory-aware-tool.ts",
    "test-memory-aware-logic": "tsx test-memory-aware-logic.ts",
    "test-memory-aware-error": "tsx test-memory-aware-error.ts",
    "test-chromadb": "tsx test-chromadb.ts",
    "test-chromadb-v1": "tsx test-chromadb-v1.ts",
    "test-chromadb-v2": "tsx test-chromadb-v2.ts",
    "test-chromadb-v2-basic": "tsx test-chromadb-v2-basic.ts",
    "test-chromadb-cloud": "tsx test-chromadb-cloud.ts",
    "test-neo4j": "tsx test-neo4j.ts",
    "test-unit": "npx jest --testPathPattern=unit --verbose",
    "test-integration": "npx jest --testPathPattern=integration --verbose",
    "test-performance": "npx jest --testPathPattern=performance --verbose",
    "test-scenarios": "npx jest --testPathPattern=scenarios --verbose",
    "test-all": "npx tsx test/run-all-tests.ts",
    "docker:build": "docker-compose build",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "docker:logs": "docker-compose logs -f",
    "docker:backup": "bash scripts/backup.sh",
    "docker:health": "bash scripts/healthcheck.sh",
    "setup": "docker-compose up -d && echo 'Waiting for services to start...' && sleep 10 && echo 'Initialization complete. Run \"npm run test\" to start the agent.'"
  },
  "keywords": [
    "sei",
    "sei-agent-kit",
    "sei ai"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@bancor/carbon-sdk": "^0.0.115-DEV",
    "@cosmjs/stargate": "^0.33.0",
    "@langchain/langgraph": "^0.2.65",
    "@langchain/openai": "^0.5.10",
    "@supabase/supabase-js": "^2.48.1",
    "axios": "^1.8.1",
    "chart.js": "^4.4.8",
    "chromadb": "^1.9.2",
    "citrex-sdk": "^1.2.2",
    "dotenv": "^16.4.7",
    "ethers": "5.7.2",
    "langchain": "^0.3.19",
    "neo4j-driver": "^5.28.1",
    "node-cron": "^3.0.3",
    "openai": "^3.3.0",
    "symphony-sdk": "^2.4.8",
    "twitter-api-v2": "^1.23.2",
    "uuid": "^11.0.5",
    "viem": "^2.23.3",
    "wagmi": "^2.14.12",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.6",
    "@types/chai": "^5.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.2",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^3.1.1"
  }
}
